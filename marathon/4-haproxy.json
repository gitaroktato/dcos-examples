{
    "id": "haproxy",
    "container": {
        "docker": {
            "image": "sargun/haproxy-demo:3",
            "network": "HOST"
        },
        "type": "DOCKER"
    },
    "cpus": 0.2,
    "env": {
        "CONFIGURL": "http://10.0.13.97:8080/haproxy.cfg"
    },
    "instances": 1,
    "mem": 128,
    "ports": [
        80,9090
    ],
    "requirePorts": true
}

